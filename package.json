{
  "name": "movie-db-api",
  "version": "1.0.0",
  "main": "src/app.ts",
  "description": "Movies API built with Nodejs, ExpressJs, Typescript and MySQL (sequelize ORM)",
  "license": "MIT",
  "keywords": [
    "nodeJs",
    "typescript",
    "express",
    "mysql",
    "sequelize",
    "api"
  ],
  "author": {
    "name": "Wladimir Ch√≥pite",
    "email": "wchopite@gmail.com"
  },
  "scripts": {
    "build": "tsc",
    "start": "node -r tsconfig-paths/register ./bin/index.js",
    "dev": "nodemon -r tsconfig-paths/register ./src/index.ts",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "eslint": "eslint . --ext .ts",
    "lint:fix": "eslint src --fix",
    "test:unit": "NODE_ENV=test jest",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config src/database/mysqlDbConnection.ts",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv-parse-variables": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.21",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv-extended": "^2.9.0",
    "dotenv-parse-variables": "^2.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^3.13.0",
    "typeorm": "^0.2.45",
    "typescript": "^4.6.2",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && npm run eslint"
    }
  }
}
